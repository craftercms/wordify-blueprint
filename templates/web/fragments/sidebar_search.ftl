<form action="/search" class="search-form">
  <div class="form-group">
    <span class="icon fa fa-search"></span>
    <input
      id="s"
      type="text"
      class="form-control"
      name="q"
      placeholder="Type a keyword and hit enter"
      value="${searchTerm!''}"
    >
  </div>
</form>